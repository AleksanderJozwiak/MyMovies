ht mr-1\">\n              Create\n            </button>\n            <button type=\"button\" @click=\"updateClik()\" v-if=\"id>-1\" class=\"btn btn-light mr-1\">\n              Update\n            </button>\n        </div>\n        <div class=\"modal-footer\">\n\n        </div>\n      </div>\n    </div>\n</div>\n</template>\n\n\n<script>\nimport axios from 'axios'\n\nexport default {\n    name: 'MovieList',\n\ndata(){\n    return {\n        movies:[],\n        modalTitle:\"\",\n        id: -1,\n        title: \"\",\n        director: \"\",\n        year: 0,\n        rate: 0,\n    };\n},\nmethods:{\n    refreshData(){\n        axios.get(\"https://localhost:7090/data\").then((response)=>{\n            this.movies = response.data;\n        });\n    },\n    addClick() {\n       this.modalTitle = \"Add movie\";\n       this.id = 0;\n       this.title = \"\";\n       this.director = \"\";\n       this.year = 0;\n       this.rate = 0;\n    },\n    editClick(mov) {\n       this.modalTitle = \"Edit movie\";\n       this.id = mov.id;\n       this.title = mov.title;\n       this.director = mov.director;\n       this.year = mov.year;\n       this.rate = mov.rate;\n    },\n    createClick() {\n        axios.post(\"https://localhost:7090/create\", {\n            title: this.title,\n            director: this.director,\n            year: this.year,\n            rate: this.rate\n        })\n        .then((response)=>{\n            alert(response.data);\n            this.refreshData();\n        });\n    },\n    //update api i tutaj 48:40\n},\nmounted:function(){\n    this.refreshData();\n}\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style scoped>\n#addBtn{\n    display: flex;\n    flex-direction: row;\n    justify-content: space-around;\n    align-items: center;\n    border: none;\n    width: 85%;\n    height: 85%;\n    border-radius: 5%;\n    width: 120px;\n    height: 40px;\n}\n.btn {\n    background-color: #4ec1de;\n    transition-duration: 0.3s;\n}\n.btn:hover {\n    background-color: gold;\n}\nheader {\n    width: 100%;\n    height: 150px;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n    background-color: #4ec1de;\n}\ntable, tbody, thead {\n    width: 100%;\n}\nth {\n    align-items: center;\n}\ntr, th {\n    display: flex;\n    justify-content: center;\n    width: 100%;\n    height: 60px;\n}\ntd {\n    width: 16.7%;\n}\n.input-group {\n    display: flex;\n    flex-direction: column;\n}\n.input-group > div{\n    display: flex;\n}\n.input-group > div > span{\n    width: 15%;\n    display: flex;\n    align-items: center;\n    justify-content: center;\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}