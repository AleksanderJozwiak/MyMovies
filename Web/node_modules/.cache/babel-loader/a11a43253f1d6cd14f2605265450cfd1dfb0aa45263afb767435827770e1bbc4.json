{"ast":null,"code":"import axios from 'axios';\nexport default {\n  name: 'MovieList',\n  data() {\n    return {\n      movies: [],\n      modalTitle: \"\",\n      id: -1,\n      title: \"\",\n      director: \"\",\n      year: 0,\n      rate: 0\n    };\n  },\n  methods: {\n    refreshData() {\n      axios.get(\"https://localhost:7090/data\").then(response => {\n        this.movies = response.data;\n      });\n    },\n    addClick() {\n      this.modalTitle = \"Add movie\";\n      this.id = 0;\n      this.title = \"\";\n      this.director = \"\";\n      this.year = 0;\n      this.rate = 0;\n    },\n    editClick(mov) {\n      this.modalTitle = \"Edit movie\";\n      this.id = mov.id;\n      this.title = mov.title;\n      this.director = mov.director;\n      this.year = mov.year;\n      this.rate = mov.rate;\n    },\n    createClick() {\n      axios.post(\"https://localhost:7090/create\", {\n        title: this.title,\n        director: this.director,\n        year: this.year,\n        rate: this.rate\n      }).then(response => {\n        alert(response.data);\n        this.refreshData();\n      });\n    }\n    //update api i tutaj 48:40\n  },\n\n  mounted: function () {\n    this.refreshData();\n  }\n};","map":{"version":3,"mappings":"AAyGA,OAAOA,KAAI,MAAO,OAAM;AAExB,eAAe;EACXC,IAAI,EAAE,WAAW;EAErBC,IAAI,GAAE;IACF,OAAO;MACHC,MAAM,EAAC,EAAE;MACTC,UAAU,EAAC,EAAE;MACbC,EAAE,EAAE,CAAC,CAAC;MACNC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,CAAC;MACPC,IAAI,EAAE;IACV,CAAC;EACL,CAAC;EACDC,OAAO,EAAC;IACJC,WAAW,GAAE;MACTX,KAAK,CAACY,GAAG,CAAC,6BAA6B,CAAC,CAACC,IAAI,CAAEC,QAAQ,IAAG;QACtD,IAAI,CAACX,MAAK,GAAIW,QAAQ,CAACZ,IAAI;MAC/B,CAAC,CAAC;IACN,CAAC;IACDa,QAAQ,GAAG;MACR,IAAI,CAACX,UAAS,GAAI,WAAW;MAC7B,IAAI,CAACC,EAAC,GAAI,CAAC;MACX,IAAI,CAACC,KAAI,GAAI,EAAE;MACf,IAAI,CAACC,QAAO,GAAI,EAAE;MAClB,IAAI,CAACC,IAAG,GAAI,CAAC;MACb,IAAI,CAACC,IAAG,GAAI,CAAC;IAChB,CAAC;IACDO,SAAS,CAACC,GAAG,EAAE;MACZ,IAAI,CAACb,UAAS,GAAI,YAAY;MAC9B,IAAI,CAACC,EAAC,GAAIY,GAAG,CAACZ,EAAE;MAChB,