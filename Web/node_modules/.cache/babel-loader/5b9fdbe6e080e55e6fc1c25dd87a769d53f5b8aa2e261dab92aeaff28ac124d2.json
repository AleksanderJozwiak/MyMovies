{"ast":null,"code":"import axios from 'axios';\nimport useValidate from '@vuelidate/core';\nimport { between, required, maxLength } from '@vuelidate/validators';\nimport { reactive, computed } from 'vue';\nexport default {\n  name: 'MovieList',\n  validations() {\n    return {\n      title: {\n        required,\n        maxLength: maxLength(200)\n      },\n      year: {\n        required,\n        between: between(1900, 2200)\n      },\n      rate: {\n        required,\n        between: between(0, 10)\n      }\n    };\n  },\n  data() {\n    return {\n      v$: useValidate(),\n      movies: [],\n      modalTitle: \"\",\n      id: -1,\n      title: \"\",\n      director: \"\",\n      year: 0,\n      rate: 0\n    };\n  },\n  methods: {\n    refreshData() {\n      axios.get(\"https://localhost:7090/data\").then(response => {\n        this.movies = response.data;\n      });\n    },\n    addClick() {\n      this.modalTitle = \"Add movie\";\n      this.id = -1;\n      this.title = \"\";\n      this.director = \"\";\n      this.year = 0;\n      this.rate = 0;\n    },\n    editClick(mov) {\n      this.modalTitle = \"Edit movie\";\n      this.id = mov.id;\n      this.title = mov.title;\n      this.director = mov.director;\n      this.year = mov.year;\n      this.rate = mov.rate;\n    },\n    createClick() {\n      this.v$.$validate();\n      if (!this.v$.$error) {\n        axios.post(\"https://localhost:7090/create\", {\n          title: this.title,\n          director: this.director,\n          year: this.year,\n          rate: this.rate\n        }).then(alert(\"Created data\"), this.refreshData());\n      } else {\n        alert(\".\");\n      }\n    },\n    updateClik() {\n      this.v$.$validate();\n      if (!this.v$.$error) {\n        axios.put(\"https://localhost:7090/update\", {\n          id: this.id,\n          title: this.title,\n          director: this.director,\n          year: this.year,\n          rate: this.rate\n        }).then(alert(\"Updated data\"), this.refreshData());\n      } else {\n        console.log(this.v$.title.required);\n      }\n    },\n    deleteClick(id) {\n      if (!confirm(\"Are you sure?\")) {\n        return;\n      }\n      axios.delete(\"https://localhost:7090/delete?id=\" + id).then(alert(\"Deleted data\"), this.id = -1, this.refreshData());\n    }\n  },\n  mounted: function () {\n    this.refreshData();\n  }\n};","map":{"version":3,"mappings":"AA6GA,OAAOA,KAAI,MAAO,OAAM;AACxB,OAAOC,WAAU,MAAO,iBAAgB;AACxC,SAASC,OAAO,EAAEC,QAAQ,EAAEC,SAAS,QAAO,uBAAsB;AAClE,SAASC,QAAQ,EAAEC,QAAO,QAAS,KAAI;AAEvC,eAAe;EACfC,IAAI,EAAE,WAAW;EAEjBC,WAAW,GAAE;IACL,OAAM;MACFC,KAAK,EAAE;QACHN,QAAQ;QACRC,SAAS,EAAEA,SAAS,CAAC,GAAG;MAC5B,CAAC;MACDM,IAAI,EAAE;QACFP,QAAQ;QACRD,OAAO,EAAEA,OAAO,CAAC,IAAI,EAAE,IAAI;MAC/B,CAAC;MACDS,IAAI,EAAE;QACFR,QAAQ;QACRD,OAAO,EAAEA,OAAO,CAAC,CAAC,EAAE,EAAE;MAC1B;IACJ;EACJ,CAAC;EAGLU,IAAI,GAAE;IACF,OAAO;MACHC,EAAE,EAAEZ,WAAW,EAAE;MACjBa,MAAM,EAAC,EAAE;MACTC,UAAU,EAAC,EAAE;MACbC,EAAE,EAAE,CAAC,CAAC;MACNP,KAAK,EAAE,EAAE;MACTQ,QAAQ,EAAE,EAAE;MACZP,IAAI,EAAE,CAAC;MACPC,IAAI,EAAE;IACV,CAAC;EACL,CAAC;EACDO,OAAO,EAAC;IACJC,WAAW,GAAE;MACTnB,KAAK,CAACoB,GAAG,CAAC,6BAA6B,CAAC,CAACC,IAAI,CAAEC,QAAQ,IAAG;QACtD,IAAI,CAACR,MAAK,GAAIQ,QAAQ,CAACV,IAAI;MAC/B,CAAC,CAAC;IACN,CAAC;IACDW,QAAQ,GAAG;MACR,IAAI,CAACR,UAAS,GAAI,WAAW;MAC7B,IAAI,CAACC,EAAC,GAAI,CAAC,CAAC;MACZ,IAAI,CAACP,KAAI,GAAI,EAAE;MACf,IAAI,CAACQ,QAAO,GAAI,EAAE;MAClB,IAAI,CAACP,IAAG,GAAI,CAAC;MACb,IAAI,CAACC,IAAG,GAAI,CAAC;IAChB,CAAC;IACDa,SAAS,CAACC,GAAG,EAAE;MACZ,IAAI,CAACV,UAAS,GAAI,YAAY;MAC9B,IAAI,CAACC,EAAC,GAAIS,GAAG,CAACT,EAAE;MAChB,IAAI,CAACP,KAAI,GAAIgB,GAAG,CAAChB,KAAK;MACtB,IAAI,CAACQ,QAAO,GAAIQ,GAAG,CAACR,QAAQ;MAC5B,IAAI,CAACP,IAAG,GAAIe,GAAG,CAACf,IAAI;MACpB,IAAI,CAACC,IAAG,GAAIc,GAAG,CAACd,IAAI;IACvB,CAAC;IACDe,WAAW,GAAG;MACV,IAAI,CAACb,EAAE,CAACc,SAAS,EAAC;MAClB,IAAI,CAAC,IAAI,CAACd,EAAE,CAACe,MAAM,EAAE;QACjB5B,KAAK,CAAC6B,IAAI,CAAC,+BAA+B,EAAE;UACxCpB,KAAK,EAAE,IAAI,CAACA,KAAK;UACjBQ,QAAQ,EAAE,IAAI,CAACA,QAAQ;UACvBP,IAAI,EAAE,IAAI,CA